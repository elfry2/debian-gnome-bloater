# Source this file in your shell's .*rc file.

alias l="ls -lah"
alias c="clear"
alias x="exit"
alias e="l3afpad"
alias n="neofetch"
alias h="htop"
alias r="ranger"
alias v="nvim"
alias pm="sudo apt"
alias pmi="sudo apt install -y"
alias fpi="flatpak install -y"
alias pms="apt search"
alias fps="flatpak search"
alias pmu="sudo apt update && sudo apt full-upgrade -y && flatpak update -y"
alias pmr="sudo apt purge"
alias fpr="flatpak uninstall"
alias pmc="sudo apt autoremove && sudo apt clean && flatpak uninstall --unused"
